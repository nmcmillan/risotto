<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">

	<RelativeLayout android:id="@+id/title_bar" style="@style/title_bar_style">
		<TextView android:id="@+id/title_bar_logo" style="@style/title_bar_logo_style" />
		<Button android:id="@+id/title_bar_button_sync" style="@style/button_title_bar_sync_style" />
	</RelativeLayout>
	<RelativeLayout android:layout_alignParentRight="true"
		android:id="@+id/wizard_schedule_review_view" android:layout_width="match_parent"
		android:layout_below="@id/title_bar" android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content"
			android:layout_alignParentLeft="true" android:layout_width="wrap_content"
			android:id="@+id/wizard_schedule_review_who_label" android:text="Who: "
			android:textSize="25sp"></TextView>
		<TextView android:layout_height="wrap_content"
			android:layout_below="@id/wizard_schedule_review_who_label"
			android:layout_width="wrap_content" android:layout_alignLeft="@id/wizard_schedule_review_who_label"
			android:id="@+id/wizard_schedule_review_what_label" android:text="What:"
			android:textSize="25sp"></TextView>
		<TextView android:layout_height="wrap_content"
			android:layout_below="@id/wizard_schedule_review_what_label"
			android:layout_width="wrap_content" android:layout_alignLeft="@id/wizard_schedule_review_what_label"
			android:id="@+id/wizard_schedule_review_when_label" android:text="When:"
			android:textSize="25sp"></TextView>
		<TextView android:layout_toRightOf="@id/wizard_schedule_review_when_label"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:text="TextView" android:layout_alignTop="@id/wizard_schedule_review_when_label"
			android:layout_alignBottom="@id/wizard_schedule_review_when_label"
			android:textSize="25sp" android:id="@+id/wizard_schedule_review_when"></TextView>
		<TextView android:layout_toRightOf="@id/wizard_schedule_review_what_label"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:text="TextView" android:layout_alignTop="@id/wizard_schedule_review_what_label"
			android:layout_alignBottom="@id/wizard_schedule_review_what_label"
			android:id="@+id/wizard_schedule_review_what" android:textSize="25sp"></TextView>
		<TextView android:layout_toRightOf="@id/wizard_schedule_review_who_label"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:text="TextView" android:layout_alignTop="@id/wizard_schedule_review_who_label"
			android:layout_alignBottom="@id/wizard_schedule_review_who_label"
			android:textSize="25sp" android:id="@+id/wizard_schedule_review_who"></TextView>
	</RelativeLayout>
	<Button android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_width="wrap_content" android:id="@+id/button_wizard_schedule_review_schedule" android:text="Schedule!" android:layout_alignParentBottom="true"></Button>

</RelativeLayout>
